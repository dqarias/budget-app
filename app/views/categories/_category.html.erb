<div id="<%= dom_id category %>">
<%= link_to category_operations_path(category) do %>
  <p>
    <strong>Name:</strong>
    <%= category.name %>
  </p>

  <p>
    <strong>Icon:</strong>
    <%= category.icon %>
  </p>

  <p>
    <strong>User:</strong>
    <%= category.user_id %>
  </p>
<% end %>
</div>
